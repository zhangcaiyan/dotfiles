alias zcy_cs2="ssh cnjobs@218.240.21.105"
alias zcy_ms1='ssh root@42.96.159.101'
alias zcy_ms='ssh zcy@42.96.159.101'

  # ls
alias ls="ls --color"
alias l="ls -lAh --color"
alias ll="ls -lFh --color"
alias la='ls -A --color'

  # cd
alias ..='cd ..'
